/button>\n                  </>\n              }\n\n              <button onClick={onClickrRemove} className=\"flex-no-shrink p-2 ml-2 border-2 rounded text-red border-red hover:text-white hover:bg-red\">Remove</button>\n            </div>\n\n          </>\n\n\n\n          :\n          <></>\n      }\n\n    </>\n\n\n\n  );\n};\n\nexport default TodoItem;\n\n",{"ruleId":"49","severity":1,"message":"50","line":4,"column":10,"nodeType":"51","messageId":"52","endLine":4,"endColumn":20},{"ruleId":"49","severity":1,"message":"53","line":10,"column":10,"nodeType":"51","messageId":"52","endLine":10,"endColumn":18},{"ruleId":"49","severity":1,"message":"54","line":10,"column":20,"nodeType":"51","messageId":"52","endLine":10,"endColumn":31},{"ruleId":"55","severity":1,"message":"56","line":114,"column":15,"nodeType":"57","endLine":114,"endColumn":181},{"ruleId":"55","severity":1,"message":"56","line":122,"column":15,"nodeType":"57","endLine":122,"endColumn":119},{"ruleId":"55","severity":1,"message":"56","line":131,"column":15,"nodeType":"57","endLine":131,"endColumn":158},{"ruleId":"55","severity":1,"message":"56","line":149,"column":15,"nodeType":"57","endLine":149,"endColumn":111},{"ruleId":"49","severity":1,"message":"58","line":1,"column":17,"nodeType":"51","messageId":"52","endLine":1,"endColumn":28},{"ruleId":"49","severity":1,"message":"59","line":1,"column":30,"nodeType":"51","messageId":"52","endLine":1,"endColumn":36},{"ruleId":"49","severity":1,"message":"60","line":4,"column":3,"nodeType":"51","messageId":"52","endLine":4,"endColumn":11},"@typescript-eslint/no-unused-vars","'faEnvelope' is defined but never used.","Identifier","unusedVar","'redirect' is defined but never used.","'useNavigate' is defined but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","'ChangeEvent' is defined but never used.","'useRef' is defined but never used.","'addTodos' is de